<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config/>
	
	<bean id="car" class="com.flytxt.training.autowire.car.Car">
		<property name="color" value="White"/>
	</bean>
	<bean id="engine" class="com.flytxt.training.autowire.car.internals.engine.impl.DieselEngine" />

	<bean id="transmission" class="com.flytxt.training.autowire.car.internals.transmission.impl.AutomaticTransmission" />

	<bean id="wheel" class="com.flytxt.training.autowire.car.externals.wheel.impl.AlloyWheel" />

	<bean id="seat" class="com.flytxt.training.autowire.car.externals.seat.impl.LeatherSeat" />
	
</beans>